{
  "manifest_version": 3,
  "name": "Alibou spotlight",
  "version": "1.0",
  "description": "Display the latest videos from my YouTube channel",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
